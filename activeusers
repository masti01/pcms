#!/bin/bash
cd ~/pw/core
time python pwb.py masti/ms-basicuserinfo.py -links:'Wikipedysta:MastiBot/Atlas/Warszawa' -ns:user -summary:"Bot uaktualnia stronę" -pt:0 -outpage:'Wikipedysta:mastiBot/Aktywni/Warszawa'
time python pwb.py masti/ms-basicuserinfo.py -links:'Wikipedysta:MastiBot/Atlas/Poznań' -ns:user -summary:"Bot uaktualnia stronę" -pt:0 -outpage:'Wikipedysta:mastiBot/Aktywni/Poznań'
time python pwb.py masti/ms-basicuserinfo.py -links:'Wikipedysta:MastiBot/Atlas/Łódź' -ns:user -summary:"Bot uaktualnia stronę" -pt:0 -outpage:'Wikipedysta:mastiBot/Aktywni/Łódź'
